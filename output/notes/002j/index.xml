  \mprset {flushleft}
  \inferrule *[right=W-Comp]{
      t : W_{c:C} A(c) \vdash  D(t) : \texttt {type} \\ 
      (r : C,\ s : A(r)\to  W_{c:C} A(c),\ s_D : \prod _{a:A(r)} D(s(a))) \\
        \vdash  \texttt {tree}_D(r,s,s_D) : D(\texttt {tree}(r,s))
  }{
    (r : C), (s : A(r)\to  W_{c:C} A(c)) \\\\
